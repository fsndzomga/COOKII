<div class="container p-3">

  <%# Search bar Partial %>
  <%= render "search_bar", meal: @meal %>
  <%# **** %>

  <%= link_to "Create a new Meal", new_meal_path, class:"btn-sign-up" %>
  <div class="container mt-3">
  <h2>All Meals</h2>
    <div class="container">
      <div class="row">
          <% @meals.each do |meal| %>
            <% if meal.user != current_user %>
              <%= render "card", meal: meal %>
            <% end %>
          <% end %>
      </div>
    </div>
</div>
